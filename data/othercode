            // Add polyline between 'From' and 'To'
            L.polyline([fromCoords, toCoords], { color: 'red', weight: 2 }).addTo(map);

            // search bar code
            <form class="form-inline">
                <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
              </form>
